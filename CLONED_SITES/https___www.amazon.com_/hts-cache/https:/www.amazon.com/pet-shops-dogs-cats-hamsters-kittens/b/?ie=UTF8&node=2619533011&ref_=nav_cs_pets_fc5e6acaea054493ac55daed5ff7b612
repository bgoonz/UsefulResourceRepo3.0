        max-width: 1540px;
        padding: 0 20px;
        position: relative;
        width: 100%;
    }

    #sobe_d_b_2-carousel {
        background-color: #fff;
        padding: 20px 20px;
        height: 100%;
        width: 100%;
    }

    #sobe_d_b_2-carousel-viewport-container {
        position: relative;
    }

    .sl-sobe-carousel-viewport {
        -ms-overflow-style: none;
        height: 100%;
        font-size: 0;
        overflow-x: auto;
        overflow-y: hidden;
        white-space: nowrap;
        width: 100%;
    }

    #sobe_d_b_2-carousel .sl-sobe-carousel-viewport-row {

<script type="text/javascript">
P.when('A', 'ready').execute(function (A) {
  
  
  
  var categoryCardV2Responsive = function() {
    var ITEM_MIN_WIDTH = 198;
    var $ = A.$;
    // The width() will carry 1 if the actually width is a float number,
    // so minus 1 in case the last one is 1 pixel wider to fit in the row
    var widgetWidth = $(".octopus-pc-category-card-v2-content").width() - 1;
    var itemCountInEachRow = Math.floor(widgetWidth / ITEM_MIN_WIDTH);
    var $items = $(".octopus-pc-category-card-v2-content .octopus-pc-category-card-v2-item");
    var $itemCount = $items.length;
    var hasSecondRow = $itemCount > itemCountInEachRow ? true : false;
    if (itemCountInEachRow > 0) {
      var singleAsinWidth = Math.floor(widgetWidth / itemCountInEachRow);
      var columnBiggerOne = widgetWidth % singleAsinWidth;
      var index = 0;
      A.map($items, function(value) {
        index++;
        if (index > itemCountInEachRow) {
          index = index - itemCountInEachRow;
        }
        if (index <= columnBiggerOne) {
          $(value).width(singleAsinWidth + 1);
        } else {
          $(value).width(singleAsinWidth);
        }
      });
    }
    
    
    $items.css('display', '');
    $items.css("border-right-width", '2px');
    
    
    var firstRowLastIndex = itemCountInEachRow - 1;
    $(".octopus-pc-category-card-v2-item:eq(" + firstRowLastIndex + ")").css("border-right-width", '0px');
    
    
    if (hasSecondRow) {
      if ($itemCount > Math.floor(itemCountInEachRow + itemCountInEachRow/2)) {
        var secondRowLastIndex = 2 * itemCountInEachRow - 1;
        $(".octopus-pc-category-card-v2-item:eq(" + secondRowLastIndex + ")").css("border-right-width", '0px');
      } else {
        for (var i = itemCountInEachRow; i < $itemCount; i++) {
         $(".octopus-pc-category-card-v2-item:eq(" + i + ")").css('display', 'none');
        }
      }
    }
  };
  categoryCardV2Responsive();

  
  A.on.resize(categoryCardV2Responsive);

  
  A.declarative('octopus-pc-category-card-v2-hover', 'mouseenter mouseleave', function(event){
    if (event.data.hasSubcategory) {
      var categoryElement = event.$currentTarget.find('.octopus-pc-category-card-v2-category');
      var categoryTitleElement = event.$currentTarget.find('.octopus-pc-category-card-v2-category-title');
      var subCategoryElement = event.$currentTarget.find('.octopus-pc-category-card-v2-subcategory');
      var time = 300;
      if (event.type == 'mouseenter') {
        A.animate(categoryElement, {
          scale: '0.5',
        }, time, 'ease-in', function(){});
        A.animate(categoryTitleElement, {
          top: '90px',
        }, time, 'ease-in', function(){});
        A.animate(subCategoryElement, {
          top: '160px',
        }, time, 'ease-in', function(){});
      } else {  
        A.animate(categoryElement, {
          scale: '1',
        }, time, 'ease-out', function(){});
        A.animate(categoryTitleElement, {
          top: '180px',
        }, time, 'ease-out', function(){});
        A.animate(subCategoryElement, {
          top: '258px',
        }, time, 'ease-out', function(){});
      }
    }
  });
});

P.when('A', 'load').execute('load-category-v2-image', function(A){
    A.loadHiResImage(A.$('.octopus-pc-category-card-v2-image'));
});
</script>







<script type="text/javascript">
    P.when('A', 'jQuery', 'ready').execute(function (A, $) {
        $('.octopus-pc-asin-block').hover(function () {
            $(this).find('.a-declarative > #octopus-quick-look-btn').removeClass("aok-hidden");
        }, function () {
            $(this).find('.a-declarative > #octopus-quick-look-btn').addClass("aok-hidden");
        });
    });
</script>




    
    
        
            
            
                
            
        
    




    
        
            
            
            
            
            
            
            
        
        
        
        
        
    

    <script type="text/javascript">

P.when('A', 'jQuery', 'ready').execute(function(A, $){
    window.registeredCountDownObj || P.register("countDown", function () {
        return {
            getCountDownObj: function(domId, toStartTime, toEndTime, timeStartText, timeEndText, dealOverText) {
                return {
                    timerId: null,
                    initEpoch: Math.floor(new Date().getTime() / 1000),

                    displayCountdown: function() {
                        var currentEpoch = Math.floor(new Date().getTime() / 1000);
                        var remainStartSeconds = toStartTime / 1000 - (currentEpoch - this.initEpoch);
                        var remainEndSeconds = toEndTime / 1000 - (currentEpoch - this.initEpoch);

                        if (remainStartSeconds >= 1) {
                            var countdownText = this.getLeftTimeString(remainStartSeconds);
                            var htmlDealTime = timeStartText + countdownText;
                            $(domId).html(htmlDealTime);
                        } else if (remainStartSeconds >= 0 && remainStartSeconds < 1) {
                            window.location.reload();
                        } else if (remainEndSeconds > 0) {
                            var countdownText = this.getLeftTimeString(remainEndSeconds);
                            var htmlDealTime = timeEndText + countdownText;
                            $(domId).html(htmlDealTime);
                        } else {
                            this.stopTimer();
                            $(domId).html(dealOverText);
                        }
                    },
                    startTimer: function() {
                        var thisObj = this;
                        if (this.timerId == null) {
                            this.timerId = A.interval(function() {
                                if (thisObj) {
                                    thisObj.displayCountdown();
                                }
                            }, 1000);
                        }
                    },
                    stopTimer: function() {
                        if (this.timerId != null) {
                            window.clearInterval(this.timerId);
                            this.timerId = null;
                        }
                    },
                    autoCompleteTimeDigit: function(timeDigit) {
                        return timeDigit < 10 ? ("0" + timeDigit) : timeDigit;
                    },
                    getLeftTimeString: function(remainSeconds) {
                        if (remainSeconds <= 0) return undefined;
                        var remainMins = Math.floor(remainSeconds / 60);
                        var minutes = this.autoCompleteTimeDigit(remainMins % 60);
                        var hours = this.autoCompleteTimeDigit(Math.floor(remainMins / 60));
                        var seconds = this.autoCompleteTimeDigit(Math.floor(remainSeconds % 60));
                        return hours + ":" + minutes + ":" + seconds;
                    },
                    init: function() {
                        this.displayCountdown();
                        this.startTimer();
                    }  
                }
            }
        };
    });

    window.registeredCountDownObj = true;
});
</script>
    <script type="text/javascript">

P.when('A', 'jQuery', 'ready').execute(function(A, $){
    window.claimPercentRefreshObj || P.register("claimPercentRefresh", function () {
        return {
            getClaimPercentRefreshObj: function(marketplaceID, dealIdStr, dealIdList, requestId, claimPercentPrefix,
            claimPercentTextPrefix) {
                return {
                    timerId: null,
                    refreshClaimPercent: function() {
                        A.ajax('/apb/page/content', {
                            method : 'post',
                            params : {
                                'handlerName' : 'OctopusLightningDealsAjaxClaimPercent',
                                'marketplaceId' : marketplaceID,
                                'requestId' : requestId,
                                'deals' : dealIdStr,
                                'ajaxCall' : 'true'
                            },
                            success : function(data) {
                                for (var dealIndex in dealIdList) {
                                    var dealId = dealIdList[dealIndex];
                                    if ( dealId in data) {
                                        var dealStatus = data[dealId];
                                        if ('claimPercent' in dealStatus) {
                                            var dealClaimPercentId = claimPercentPrefix + dealId;
                                            $(dealClaimPercentId).css({'width' : dealStatus['claimPercent']});
                                        }
                                        if ('claimPercentText' in dealStatus) {
                                            var dealClaimPercentTextId = claimPercentTextPrefix + dealId;
                                            $(dealClaimPercentTextId).html(dealStatus['claimPercentText']);
                                        }
                                    }
                                }
                            },
                            error : function() {
                                console.info("Failed");
                            }
                        });
                    },
                    startTimer: function() {
                        var thisObj = this;
                        if (this.timerId == null) {
                            this.timerId = A.interval(function() {
                                if (thisObj) {
                                    thisObj.refreshClaimPercent();
                                }
                            }, 60000);
                        }
                    },
                    init: function() {
                        this.refreshClaimPercent();
                        this.startTimer();
                    }
                }
            }
        };
    });

    window.claimPercentRefreshObj = true;
});
</script>

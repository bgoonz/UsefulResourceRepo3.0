{
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "indent": ["error", 2],
        "no-var": "error",
        "comma-dangle": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "no-restricted-syntax": 0,
        "no-multi-assign": 0,
        "no-plusplus": 0,
        "no-mixed-operators": 0
    },
    "env": {
        "browser": true
    }
}

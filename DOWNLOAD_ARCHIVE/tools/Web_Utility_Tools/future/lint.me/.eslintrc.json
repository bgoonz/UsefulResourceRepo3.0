{
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "indent": ["error", 4],
        "no-var": "error",
        "comma-dangle": ["error", "never"],
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
        "global-require": 0,
        "consistent-return": 0,
        "no-restricted-syntax": 0
    },
    "env": {
        "browser": true
    }
}

{
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "no-var": "error",
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
        "global-require": 0,
        "class-methods-use-this": 0,
        "no-restricted-syntax": [
          "error",
          "ForInStatement"
        ]
    },
    "env": {
        "browser": true,
        "jasmine": true
    }
}
